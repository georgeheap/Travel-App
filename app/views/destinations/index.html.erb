<div class="main">
  <div class="heading">
    <h1>Destinations</h1>
  </div>
  <div class="col-md-11 col-md-offset-1">
    <div class="dest-main">
      <% @destinations.each do |destination| %>
          <%= link_to image_tag("#{destination.default_img}", alt:"#{destination.name}", class: "dest_img", id: "#{destination.name}"), destination %>
      <% end %>
    </div>
  </div>
</div>
